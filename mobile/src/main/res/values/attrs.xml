<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SetView">
        <attr name="contentLayout" format="reference" />
        <attr name="titleEx" format="string" />
        <attr name="summary" format="string" />
        <attr name="summary2" format="string" />
        <attr name="type">
            <enum name="buttonB" value="0" />
            <enum name="selectB" value="1" />
            <enum name="checkB" value="2" />
            <enum name="switchB" value="3" />
            <enum name="inputB" value="4" />
        </attr>
        <attr name="listName" format="reference" />
        <attr name="listValue" format="reference" />
    </declare-styleable>

    <declare-styleable name="Theme">
        <attr name="customImageViewStyle" format="reference" />
    </declare-styleable>
    <!-- 自定义圆角ImageView -->
    <declare-styleable name="CustomImageView">
        <attr name="circle" format="boolean" />
        <attr name="radius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="MultipleTextView">

        <!--前缀文字-->
        <attr name="prefixText" format="string" />
        <!--前缀文字颜色-->
        <attr name="prefixTextColor" format="color" />

        <!--前缀内容文字-->
        <attr name="contentText" format="string" />
        <!--前缀内容文字颜色-->
        <attr name="contentTextColor" format="color" />

        <!--后缀文字-->
        <attr name="suffixText" format="string" />
        <!--后缀文字颜色-->
        <attr name="suffixTextColor" format="color" />

    </declare-styleable>

</resources>